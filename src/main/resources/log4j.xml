<Configuration status="debug" name="beauty" packages="" advertiser="multicastdns">
  <Appenders>
    <Console name="stdout" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
    </Console>
    <File name="fout" fileName="beauty.log" append="true"
      advertiseURI="./beauty.log" advertise="true">
      <PatternLayout>
        <Pattern>%d{yyyy-MM-dd HH:mm:ss} %-5p %m%nw</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="fout"/>
      <AppenderRef ref="stdout" />
    </Root>
  </Loggers>
</Configuration>
